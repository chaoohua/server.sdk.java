{
	"getToken": {
		"docs": "http://rongcloud.cn/docs/server.html#user_get_token",
		"url": "user/getToken",
		"params": {
			"user": {
				"id": {
					"verify": {
						"maxLen": "32",
						"lenInvalid": "10003"
					}
